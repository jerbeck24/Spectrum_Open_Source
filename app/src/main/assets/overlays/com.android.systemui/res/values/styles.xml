<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RecentBaseStyle" parent="@*android:style/Theme.Material">
        <item name="android:popupMenuStyle">@*android:style/Widget.Material.ListPopupWindow</item>
    </style>
    <style name="RecentsStyle" parent="@*android:style/Theme.Material.Wallpaper.NoTitleBar" />

    <style name="systemui_theme" parent="@*android:style/Theme.Material">
        <!-- Switch color in QS -->
        <item name="android:colorControlActivated">@android:color/holo_blue_light</item>
        <item name="android:colorPrimary">@android:color/holo_blue_light</item>
    </style>
    <style name="Theme.SystemUI.Dialog" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:textColor">@*android:color/Teal_700</item>
        <item name="android:colorControlActivated">#ffffffff</item>
        <item name="android:colorPrimary">@android:color/holo_blue_light</item>
    </style>
    <style name="Theme.SystemUI.Dialog.Alert" parent="@*android:style/Theme.Material.Dialog.Alert" />
    <style name="Theme.AlertDialogHost" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:alertDialogTheme">@style/Theme.SystemUI.Dialog.Alert</item>
    </style>
    <style name="TextAppearance.Material.Notification.Parenthetical" parent="@*android:style/TextAppearance.Material.Notification">
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Widget.TransportControl.SeekBar" parent="@*android:style/Widget.Material.SeekBar" />
    <style name="PasswordTheme" parent="@*android:style/Theme.Material">
        <item name="android:colorControlNormal">#80ffffff</item>
        <item name="android:colorControlActivated">#80ffffff</item>
    </style>



    <style name="TextAppearance.NotificationGuts" parent="@android:style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/Teal_700</item>
        <item name="android:fontFamily">roboto-regular</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Header" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textSize">12.0sp</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Secondary" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:alpha">1.0</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Primary" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:textSize">16.0sp</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="TextAppearance.NotificationGuts.Radio" parent="@style/TextAppearance.NotificationGuts">
        <item name="android:alpha">1.0</item>
    </style>

        <style name="DualToneLightTheme">
            <item name="android:fillColor">#ffffffff</item>
        </style>
        <style name="DualToneDarkTheme">
            <item name="android:fillColor">#ff000000</item>
        </style>



</resources>
